<%- include("../partials/header") %>

<div class="main-body">
    <div class="element">
        <h2>Production Calendar</h2>
        <div class="fromDB" style="display: none;">
            <p id="foundDates">
                <%= JSON.stringify(foundDates) %> 
            </p>
        </div>
        <table>
            <thead>
                <tr class="date2">
                    <th colspan="7"></th>
                </tr>
                <tr class="date3">
                    <th colspan="2" class="prev2">&#11013</th>
                    <th colspan="3" id="today"></th>
                    <th colspan="2" class="next2">&#10145</th>
                </tr>
                <tr>
                    <th>Sun</th>
                    <th>Mon</th>
                    <th>Tue</th>
                    <th>Wed</th>
                    <th>Thu</th>
                    <th>Fri</th>
                    <th>Sat</th>
                </tr>
            </thead>
            <tbody class="days2">
                
            </tbody>
        </table>
        <hr>
        <p>
            <a class="button" href="/productionCalendar/new">Add New Production Shift</a>
        </p>
    </div>
</div>



<%- include("../partials/sidebar") %>
<%- include("../partials/footer") %>