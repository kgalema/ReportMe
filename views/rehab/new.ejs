<%- include("../partials/header") %>


    <div class="main-body">

        <div class="element">
            <h2>Rehabilitated Panel: <%= foundRed.panel %> of <%= foundRed.section.name %> </h2>
            <form action="/sections/<%= foundRed.section.id %>/redPanels/<%= foundRed._id %>/rehabedPanels" method="POST">
                <table class="form-row">
                    <tr>
                        <td class="form-row">
                            <label for="date-declared">Rehabilitated Date:</label>
                        </td>
                        <td class="form-row">
                            <input type="date" id="date-rehabilitated" name="rehabed[rehabDate]" required>
                        </td>
                    </tr>
                    <tr>
                        <td class="form-row" colspan="2">
                            <input style="width: 100%;" type="submit" value="Submit">
                        </td>
                    </tr>
                </table>
            </form>
            <hr>
            <a href="/redPanel">Back</a>
        </div>
    </div>


<%- include("../partials/sidebar") %>
